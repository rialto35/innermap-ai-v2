# CrewAI Configuration
CREWAI_TELEMETRY_OPT_OUT=true
CREWAI_MAX_ITERATIONS=10
CREWAI_TIMEOUT_SECONDS=300

# Analysis Configuration
ANALYSIS_CACHE_TTL=3600
ANALYSIS_MAX_TOKENS=4000

