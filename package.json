{
  "name": "innermap-ai",
  "version": "1.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "generate-heroes": "node src/scripts/generateHeroImages.js",
    "typecheck": "tsc --noEmit",
    "typecheck:ci": "node scripts/run-typecheck.cjs",
    "tree:ci": "node scripts/print-tree.cjs . 3",
    "verify": "npm run typecheck:ci && npm run build",
    "verify-env": "node scripts/verify-env.mjs",
    "prebuild": "node scripts/verify-env.mjs",
    "build:assets": "tsx scripts/build-asset-manifest.ts",
    "postbuild": "npm run build:assets",
    "test:e2e": "NEXT_PUBLIC_E2E=1 playwright test",
    "test:e2e:ui": "NEXT_PUBLIC_E2E=1 playwright test --ui",
    "test:e2e:headed": "NEXT_PUBLIC_E2E=1 playwright test --headed",
    "test:e2e:update": "NEXT_PUBLIC_E2E=1 playwright test --update-snapshots",
    "test:e2e:preview": "NEXT_PUBLIC_E2E=1 PW_BASE_URL=https://innermap-ai-v2-git-main-your-username.vercel.app playwright test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@hookform/resolvers": "^5.2.2",
    "@notionhq/client": "^4.0.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "manseryeok": "^1.0.1",
    "next": "^15.5.5",
    "next-auth": "^4.24.11",
    "openai": "^5.23.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-markdown": "^10.1.0",
    "react-to-print": "^3.1.1",
    "recharts": "^3.2.1",
    "stripe": "^14.8.0",
    "swr": "^2.3.6",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "msw": "^2.11.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^2.1.9"
  }
}
